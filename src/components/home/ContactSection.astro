---
import ContactFormComponent from "../contact/ContactFormComponent";
---

<div class="contact-container">
  <ContactFormComponent client:load />
</div>

<style>
  .contact-container {
    max-width: 450px;
    width: 100%;
    margin: auto;
    text-align: center;
    border-radius: 1em;

    span {
      display: inline-block;
      padding: 0.3em;
      font-size: 2.2em;
      font-weight: 600;
    }
  }
</style>
